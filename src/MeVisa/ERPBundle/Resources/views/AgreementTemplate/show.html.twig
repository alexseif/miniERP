{% extends ':default:admin.html.twig' %}

{% block content %}
    <h1>AgreementTemplate</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Agreement</th>
                <td>{{ entity.agreement }}</td>
            </tr>
            <tr>
                <th>Signee</th>
                <td>{{ entity.signee }}</td>
            </tr>
            <tr>
                <th>Signeetitle</th>
                <td>{{ entity.signeeTitle }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('agreement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('agreement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
