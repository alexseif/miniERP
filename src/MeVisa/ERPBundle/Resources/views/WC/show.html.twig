{% extends ':default:admin.html.twig' %}

{% block content %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="panel-title">WC</span>
        </div>
        <table class="record_properties">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ entity.id }}</td>
                </tr>
                <tr>
                    <th>Get</th>
                    <td>{{ entity.get }}</td>
                </tr>
                <tr>
                    <th>Post</th>
                    <td>{{ entity.post }}</td>
                </tr>
                <tr>
                    <th>Createdat</th>
                    <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
                </tr>
            </tbody>
        </table>
    </div>
    <ul class="record_actions">
        <li>
            <a href="{{ path('wc') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('wc_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
